apikey = "<%= @apikey %>"

[roles] = ["ff-isucon:app"]

[plugin.metrics.linux]
command = "/usr/local/bin/mackerel-plugin-linux"
type = "metric"

[plugin.metrics.inode]
command = "/usr/local/bin/mackerel-plugin-inode"

[plugin.metrics.multicore]
command = "/usr/local/bin/mackerel-plugin-multicore"

[plugin.metrics.nginx]
command = "/usr/local/bin/mackerel-plugin-nginx -port=80"

[plugin.metrics.mysql]
command = "/usr/local/bin/mackerel-plugin-mysql -username isucon -password isucon"

